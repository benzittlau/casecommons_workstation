<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN"
 "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
 <plist version="1.0">
 <dict>
  <key>StandardOutPath</key>
  <string>/tmp/rebrewstrap.log</string>
  <key>StandardErrorPath</key>
  <string>/tmp/rebrewstrap.log</string>
  <key>Label</key>
  <string>com.pivotal.rebrewstrap</string>
  <key>UserName</key>
  <string><%= WS_USER %></string>
  <key>ProgramArguments</key>
  <array>
   <string>/usr/local/bin/rebrewstrap</string>
  </array>
  <key>EnvironmentVariables</key>
  <dict>
    <key>PATH</key>
    <string>/Users/pivotal/.rvm/gems/ruby-1.9.3-p0/bin:/Users/pivotal/.rvm/gems/ruby-1.9.3-p0@global/bin:/Users/pivotal/.rvm/rubies/ruby-1.9.3-p0/bin:/Users/pivotal/.rvm/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
  </dict>
  <key>LowPriorityIO</key>
  <true/>
  <key>Nice</key>
  <integer>1</integer>
  <key>StartCalendarInterval</key>
  <dict>
   <key>Hour</key>
   <integer>1</integer>
   <key>Minute</key>
   <integer><%= rand(60) %></integer>
  </dict>
 </dict>
 </plist>
